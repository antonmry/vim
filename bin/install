cd ~/.vim_antonmry_runtime

echo 'set runtimepath^=~/.vim_antonmry_runtime
source ~/.vim_antonmry_runtime/vimrc/basic.vim
source ~/.vim_antonmry_runtime/vimrc/filetypes.vim
source ~/.vim_antonmry_runtime/vimrc/plugins.vim
source ~/.vim_antonmry_runtime/vimrc/extended.vim
try
source ~/.vim_antonmry_runtime/custom_config.vim
catch
endtry' > ~/.vimrc.antonmry

echo "Installed w/o any issues! Start producing your Go magic now :-)"
echo "Hint: vim -u ~/.vimrc.antonmry"
